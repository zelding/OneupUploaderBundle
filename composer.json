{
    "name": "zedling/uploader-bundle",
    "type": "symfony-bundle",
    "description": "Handles multi file uploads in Symfony2. Features included: Chunked upload, Orphans management, Gaufrette support. Forked from https://github.com/1up-lab/OneupUploaderBundle",
    "keywords": ["fileupload", "upload", "FineUploader", "blueimp", "jQuery File Uploader", "YUI3 Uploader", "Uploadify", "FancyUpload", "MooUpload", "Plupload", "Dropzone"],
    "homepage": "",
    "license": "MIT",
    "authors": [
        {
            "name": "Jim Schmid",
            "email": "js@1up.io",
            "homepage": "http://1up.io",
            "role": "Developer"
        }, {
            "name": "Kristof Dekany",
            "email": "gammaray.zedling@gmail.com",
            "homepage": "http://lyozsi.net",
            "role": "Developer"
        }
    ],

    "support": {
        "email": "gammaray.zedling@gmail.com",
        "doc": "https://github.com/zelding/OneupUploaderBundle/blob/master/Resources/doc/index.md"
    },

    "require": {
        "php":">=5.4",
        "symfony/framework-bundle": "^2.4.0|~3.0",
        "symfony/finder": "^2.4.0|~3.0"
    },

    "require-dev": {
        "amazonwebservices/aws-sdk-for-php": "1.5.*",
        "knplabs/gaufrette": "0.2.*@dev",
        "symfony/class-loader": "2.*|~3.0",
        "symfony/security-bundle": "2.*|~3.0",
        "sensio/framework-extra-bundle": "2.*|~3.0",
        "symfony/browser-kit": "2.*|~3.0",
        "phpunit/phpunit": "~4.4",
        "oneup/flysystem-bundle": "^1.2"
    },

    "replace": {
        "oneup/uploader-bundle": "1.*"
    },

    "suggest": {
        "knplabs/knp-gaufrette-bundle": "0.1.*",
        "oneup/flysystem-bundle": "^1.2"
    },

    "autoload": {
        "psr-0": { "Oneup\\UploaderBundle": "" }
    }
}
